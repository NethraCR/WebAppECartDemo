<ul class="nav navbar-nav navbar-right">
    <li>
        @if (Session["CartCounter"] == null)
        {
            @Html.ActionLink("Cart", "ShoppingCart", "Shopping", new { id = "cartitem" })
        }
        else
        {
            @Html.ActionLink("Cart("+Session["CartCounter"]+")", "ShoppingCart", "Shopping", new { id = "cartitem" })
        }
    </li>
</ul>
